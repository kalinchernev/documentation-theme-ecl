<div class="ecl-u-clearfix">
  <% members.forEach(function(member) { %>
    <div class='ecl-u-clearfix ecl-u-pv-xs' id='<%- slug(member.namespace) %>'>
      <div class="ecl-u-clearfix small pointer toggle-sibling">
        <div class="py1 contain">
            <a class='icon pin-right py1 dark-link caret-right'>â–¸</a>
            <span class='code strong strong truncate'><%= shortSignature(member) %></span>
        </div>
      </div>
      <div class="ecl-u-clearfix display-none toggle-target">
        <%= renderSection({
          section: member,
          renderSection: renderSection,
          renderParamProperty: renderParamProperty,
     	  nested: true
        }) %>
      </div>
    </div>
  <% }) %>
</div>
