{
  "name": "documentation-theme-ecl",
  "private": true,
  "version": "1.0.2",
  "description": "ECL theme for documentation.js",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./example/app",
    "build": "documentation build ./src --theme . -f html -o ./example/app",
    "bundle": "webpack",
    "develop": "run-p develop:*",
    "develop:builder": "documentation build --watch ./src --theme . -f html -o ./example/app",
    "develop:server": "reload -d ./example/app",
    "develop:js": "webpack -w",
    "lint": "eslint .",
    "start": "run-s clean bundle build develop"
  },
  "dependencies": {
    "anchor-js": "4.1.0",
    "concat-stream": "1.6.0",
    "documentation": "5.3.5",
    "github-slugger": "1.2.0",
    "highlight.js": "9.12.0",
    "lodash": "4.17.5",
    "vinyl": "2.1.0",
    "vinyl-fs": "3.0.2"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "eslint": "4.17.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.43.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "npm-run-all": "4.1.2",
    "reload": "2.2.2",
    "webpack": "3.11.0"
  }
}
