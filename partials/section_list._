<dl class="ecl-accordion" role="presentation">
  <% members.forEach(function(member) { %>
    <dt role="heading">
      <button id='<%- slug(member.namespace) %>' class="ecl-accordion__header" aria-controls='<%- slug(member.namespace) %>-panel' aria-expanded="true">
        <%= shortSignature(member) %>
      </button>
    </dt>
    <dd id='<%- slug(member.namespace) %>-panel' role="region" aria-labelledby='<%- slug(member.namespace) %>' aria-hidden="false">
      <%= renderSection({
        section: member,
        renderSection: renderSection,
        renderParamProperty: renderParamProperty,
        nested: true
      }) %>
    </dd>
  <% }) %>
</dl>
