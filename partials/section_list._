<section class='ecl-u-clearfix ecl-u-pt-xs'>
  <% members.forEach(function(member) { %>
    <%= shortSignature(member) %>
    <div id='<%- slug(member.namespace) %>-panel'>
      <%= renderSection({
        section: member,
        renderSection: renderSection,
        renderParamProperty: renderParamProperty,
        nested: true
      }) %>
    </div>
  <% }) %>
</section>
